#include <stdio.h>
#include <stdbool.h>
#include <unistd.h>
#include "esp_wifi.h"


void setup(void) {
	esp_netif_init();
    esp_netif_create_default_wifi_sta();

    wifi_init_config_t cfg = WIFI_INIT_CONFIG_DEFAULT();
    
ESP_ERROR_CHECK(esp_wifi_init(&cfg));
}


void app_main(void)
{
	



    while (true) {
        printf("Hello from app_main!\n");
        sleep(1);
    }
}
