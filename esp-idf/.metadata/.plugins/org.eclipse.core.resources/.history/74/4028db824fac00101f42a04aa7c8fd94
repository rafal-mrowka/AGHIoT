#include "esp_event.h"
#include "esp_netif.h"
#include "nvs_flash.h"
#include "esp_wifi.h"
#include "esp_err.h"
#include "protocol_example_common.h";
#include <stdio.h>
#include <stdbool.h>
#include <unistd.h>

void app_main(void)
{
	nvs_flash_init();
	esp_netif_init();
	esp_event_loop_create_default();
	
	example_connect();

    while (true) {
        printf("Hello from app_main!\n");
        sleep(1);
    }
}
